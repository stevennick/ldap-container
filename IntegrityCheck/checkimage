echo ======== check image registry.twcc.ai/ngc/nvidia/caffe-18.08-py2-v1:latest
docker run -it --rm --entrypoint /bin/bash -d --name caffe registry.twcc.ai/ngc/nvidia/caffe-18.08-py2-v1:latest
docker cp check.sh caffe:/tmp/
docker cp IntegrityCheck.md5sum caffe:/tmp/
docker exec -it caffe /tmp/check.sh
docker stop caffe

